<div class="col-xl-12">
    <div class="card user-data-table text-center">
        <div class="card-header pb-0 d-block d-sm-flex border-0">
            <div class="me-3">
                <h4 class="card-title mb-2">Latest Transaction</h4>
                <span class="fs-12">Lorem ipsum dolor sit amet, consectetur</span>
            </div>
            <div class="card-tabs mt-3 mt-sm-0">
                <ul class="nav nav-tabs" role="tablist">
                    <!-- Add tabs if needed -->
                </ul>
            </div>
        </div>
        <div class="card-body tab-content p-0">
            <div class="d-flex flex-column align-items-center mb-3">
                <div class="mb-2">
                    <input type="number" class="form-control" placeholder="Amount" id="investmentAmount" style="width: 120px;">
                </div>
                <div class="mb-2">
                    <select class="form-select" id="cryptoSelect">
                        <option value="bitcoin">Bitcoin</option>
                        <option value="ethereum">Ethereum</option>
                        <!-- Add more crypto options as needed -->
                    </select>
                </div>
                <div>
                    <!-- Use radio buttons for actions -->
                    <input type="radio" name="actionRadio" value="buy" id="buyRadio">
                    <label for="buyRadio">Buy</label>
                    
                    <input type="radio" name="actionRadio" value="sell" id="sellRadio">
                    <label for="sellRadio">Sell</label>
                </div>
                <div>
                    <!-- Separate submit button -->
                    <button class="btn btn-primary" id="submitButton">Submit</button>
                </div>
            </div>
            <canvas id="cryptoChart" width="800" height="400"></canvas>
        </div>
    </div>
</div>


<script>
    document.addEventListener('DOMContentLoaded', function () {
    function submitInvestment(action) {
        const amountToInvest = parseFloat(document.getElementById('investmentAmount').value);
        const cryptoSelected = document.getElementById('cryptoSelect').value;

        fetch('invest/new', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'X-Requested-With': 'XMLHttpRequest',
            },
            body: JSON.stringify({
                crypto: cryptoSelected,
                amount: amountToInvest,
                action: action,
            }),
        })
        .then(response => response.json())
        .then(data => {
            const clientId = data.clientId;

            console.log('Response from server:', data);

            // Update the comptecourant solde after submitting the investment
            fetch('/decrement-solde', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                    'X-Requested-With': 'XMLHttpRequest',
                },
                body: JSON.stringify({
                    clientId: 2,
                    solde: data.solde,
                }),
            })
            .then(response => response.json())
            .then(soldeData => {
                console.log('Comptecourant Solde Updated:', soldeData);

                // Decrement the solde by the invested amount
                fetch('/decrement-solde', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'X-Requested-With': 'XMLHttpRequest',
                    },
                    body: JSON.stringify({
                        clientId: 12,
                        amount: amountToInvest,
                    }),
                })
                .then(response => response.json())
                .then(decrementData => {
                    console.log('Comptecourant Solde Decremented:', decrementData);
                    // Handle the updated solde and decrement data as needed
                })
                .catch(error => {
                    console.error('Error decrementing Comptecourant Solde:', error);
                    // Handle errors if any
                });
            })
            .catch(error => {
                console.error('Error updating Comptecourant Solde:', error);
                // Handle errors if any
            });

        })
        .catch(error => {
            console.error('Error:', error);
            // Handle errors if any
        });
    }

    // Add event listeners to the Buy and Sell radio buttons
    document.querySelectorAll('input[name="actionRadio"]').forEach(function(radio) {
        radio.addEventListener('change', function() {
            submitInvestment(this.value);
        });
    });

    // Add event listener to the "Submit" button
    document.getElementById('submitButton').addEventListener('click', function () {
        const selectedOption = document.querySelector('input[name="actionRadio"]:checked').value;
        submitInvestment(selectedOption);
    });


        const apiUrl = 'https://api.coingecko.com/api/v3/coins/bitcoin/market_chart';
        const params = {
            vs_currency: 'usd',
            days: 7,
            interval: 'daily'
        };

        fetch(`${apiUrl}?${new URLSearchParams(params)}`)
        .then(response => response.json())
        .then(data => {
            const timestamps = data.prices.map(entry => new Date(entry[0]).toLocaleDateString());
            const prices = data.prices.map(entry => entry[1]);

            const ctx = document.getElementById('cryptoChart').getContext('2d');
            new Chart(ctx, {
                type: 'line',
                data: {
                    labels: timestamps,
                    datasets: [{
                        label: 'Bitcoin Price (USD)',
                        data: prices,
                        borderColor: 'blue',
                        borderWidth: 1
                    }]
                }
            });
        })
        .catch(error => console.error('Error fetching crypto data:', error));
    });
</script>
	











	



