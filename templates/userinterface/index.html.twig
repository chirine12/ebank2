    <script>
        const invoicesData = {{ invoicesData|json_encode|raw }};

        function renderInvoices() {
            const invoicesContainer = document.getElementById('RecentActivityContent');

            invoicesData.forEach((invoice, index) => {
                const invoiceItem = document.createElement('div');
                invoiceItem.className = 'd-flex py-sm-3 py-1 px-3 align-items-center student';

                invoiceItem.innerHTML = `
                    <span class="me-3">
                        <img src="${invoice.profilePic}" alt="" width="50">
                    </span>
                    <div>
                        <h6 class="font-w500 fs-16 mb-0"><a href="${invoice.profileLink}">${invoice.clientName}</a></h6>
                        <span class="fs-14 font-w400">${invoice.clientRole}</span>
                    </div>
                    <div class="price ms-auto">
                        <a href="#">
                            ${invoice.invoiceAmount}
                        </a>		
                    </div>
                `;

                invoicesContainer.appendChild(invoiceItem);
            });
        }

        document.addEventListener('DOMContentLoaded', renderInvoices);
